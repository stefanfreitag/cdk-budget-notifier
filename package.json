{
  "name": "@stefanfreitag/aws-budget-notifier",
  "version": "0.3.1",
  "main": "src/index.ts",
  "types": "src/index.d.ts",
  "bin": {
    "budget_notifier": "bin/budget_notifier.js"
  },
  "author": {
    "name": "Stefan Freitag",
    "email": "stefan@stefreitag.de"
  },
  "repository": {
    "url": "https://github.com/stefanfreitag/cdk-budget-notifier.git",
    "type": "git"
  },
  "keywords": [
    "aws",
    "cdk",
    "budget"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "compile": "jsii --silence-warnings=reserved-word && jsii-docgen",
    "watch": "jsii -w --silence-warnings=reserved-word",
    "package": "jsii-pacmak",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "release:jsii": "jsii-release",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run",
    "cdk": "cdk",
    "docgen": "jsii-docgen",
    "compat": "npx jsii-diff npm:$(node -p \"require('./package.json').name\") -k --ignore-file .compatignore || (echo \"\nUNEXPECTED BREAKING CHANGES: add keys such as 'removed:constructs.Node.of' to .compatignore to skip.\n\" && exit 1)"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.70.0",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.8",
    "aws-cdk": "1.70.0",
    "jest": "26.5.3",
    "jsii": "1.13.0",
    "jsii-diff": "1.14.0",
    "jsii-docgen": "1.4.72",
    "jsii-pacmak": "1.14.0",
    "jsii-release": "0.2.3",
    "release-it": "14.1.0",
    "ts-jest": "26.4.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "@aws-cdk/aws-budgets": "1.70.0",
    "@aws-cdk/aws-iam": "^1.67.0",
    "@aws-cdk/aws-sns": "^1.67.0",
    "@aws-cdk/core": "1.70.0"
  },
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-budget-notifier",
        "module": "cdk_budget_notifier"
      },
      "java": {
        "package": "io.github.stefanfreitag.cdk.budgetnotifier",
        "maven": {
          "groupId": "io.github.stefanfreitag",
          "artifactId": "cdkBudgetNotifier"
        }
      },
      "dotnet": {
        "namespace": "De.Freitag.Stefan.Aws.Cdk",
        "packageId": "CdkBudgetNotifier"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "peerDependencies": {
    "constructs": "3.0.4",
    "@aws-cdk/aws-budgets": "1.70.0",
    "@aws-cdk/core": "1.70.0",
    "@aws-cdk/aws-sns": "^1.67.0",
    "@aws-cdk/aws-iam": "^1.67.0"
  }
}
